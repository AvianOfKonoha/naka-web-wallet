{
  "name": "vue-client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "deploy:origin": "gh-pages -d dist",
    "deploy:naka": "gh-pages -d dist --remote naka",
    "deploy:both": "pnpm run deploy:origin && pnpm run deploy:naka"
  },
  "dependencies": {
    "@metamask/sdk": "^0.33.0",
    "js-sha3": "^0.9.3",
    "pinia": "^3.0.3",
    "rlp": "^3.0.0",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "vue3-toastify": "^0.2.8",
    "web3": "^4.16.0"
  },
  "devDependencies": {
    "@metamask/providers": "^22.1.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.16.4",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/tsconfig": "^0.7.0",
    "gh-pages": "^6.3.0",
    "npm-run-all2": "^7.0.2",
    "sass": "^1.89.2",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vue-tsc": "^2.2.12"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "bracketSpacing": false,
    "arrowParens": "always",
    "proseWrap": "preserve",
    "trailingComma": "none"
  }
}
